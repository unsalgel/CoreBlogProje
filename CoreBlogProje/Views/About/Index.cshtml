@using EntityLayer.Concrete
@model List<About>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/UserLayout.cshtml";
}

<div class="container">
<br />
    <div class="col-lg-12">
        @foreach (var item in Model)
        {
            <div class="blog-grid-top text-lg-center">
                <div>
                    <a href="single.html">
                        <img src="@item.AboutImage1" class="img-fluid" alt="">
                    </a>
                </div>
                <h3>
                    @item.AboutDetails1
                </h3>
                <p>
                    @item.AboutDetails2
                </p>

            </div>
        }
    </div>
</div>
